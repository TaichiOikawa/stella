{
  "name": "stella",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write .",
    "markuplint": "markuplint \"./**/*.tsx\"",
    "secretlint": "secretlint --maskSecrets --secretlintignore .gitignore \"**/*\"",
